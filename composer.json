{
    "name": "devert/phpunit-reporter",
    "description": "This is a package to process xml report of junit or phpunit test executions and display the result on a google spreadsheet",
    "type": "library",
    "licence": "MIT",
    "keywords": [
        "phpunit",
        "junit",
        "testing",
        "report",
        "google spreadsheet"
    ],
    "bin": [
        "report"
    ],
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "authors": [
        {
            "name": "Akongnwi Devert",
            "email": "akongnwi02@gmail.com"
        }
    ],
    "require": {
            "php": ">=7.0",
        "phpoffice/phpspreadsheet": "^1.6",
        "google/apiclient": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Devert\\": "src/"
        }
    }
}
